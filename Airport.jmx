            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import java.time.LocalDateTime
import java.time.format.DateTimeFormatter

def formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&quot;)
def startDateTime = LocalDateTime.parse(vars.get(&quot;startDate&quot;), formatter)
def endDateTime = LocalDateTime.parse(vars.get(&quot;endDate&quot;), formatter)

def jsonResponse = new JsonSlurper().parse(prev.getResponseData())
def array = jsonResponse.array

array.eachWithIndex { element, index -&gt;
     def dateTimeString = element.t_st
     def dateTime = LocalDateTime.parse(dateTimeString, formatter)
     assert dateTime.isAfter(startDateTime) &amp;&amp; dateTime.isBefore(endDateTime) : 
            &quot;Элемент $index: Время ${dateTimeString} не находится между ${startDateTime} и ${endDateTime}&quot;
 }

  
</stringProp>
